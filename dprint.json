{
  "incremental": true,
  "lineWidth": 80,
  "useTabs": false,
  "json": {
    "indentWidth": 2,
    "useTabs": false
  },
  "markdown": {
    "lineWidth": 120,
    "newLineKind": "lf"
  },
  "toml": {},
  "dockerfile": {},
  "includes": [
    "./**/{dockerfile,Dockerfile}",
    "./**/*.{ts,tsx,js,jsx,cjs,mjs,cts,mts,json,md,toml,dockerfile}"
  ],
  "excludes": [
    "./common/**",
    "./predators2/server/generated/**",
    "./predators2/datadb/generated/**",
    "**/deployment",
    "**/generated",
    "**/*-lock.json",
    "**/.next",
    "**/.nyc_output",
    "**/.purgatory",
    "**/_purgatory",
    "**/*purgatory",
    "**/dist",
    "**/node_modules",
    "**/public",
    "**/purgatory",
    "**/static",
    "**/temp"
  ],
  "prettier": {
    "bracketSpacing": true,
    "endOfLine": "auto",
    "printWidth": 80,
    "semi": true,
    "singleQuote": false,
    "tabWidth": 2,
    "trailingComma": "all",
    "useTabs": false
  },
  "plugins": [
    "https://plugins.dprint.dev/json-0.15.3.wasm",
    "https://plugins.dprint.dev/markdown-0.13.3.wasm",
    "https://plugins.dprint.dev/toml-0.5.4.wasm",
    "https://plugins.dprint.dev/dockerfile-0.2.3.wasm",
    "https://plugins.dprint.dev/prettier-0.11.0.json@385edfc0e1212b3be6412cda49322ecd62cb38ac71b9b463869bebf23a4767e3"
  ]
}
