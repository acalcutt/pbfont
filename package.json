{
  "name": "@jsse/pbfont",
  "type": "module",
  "version": "0.0.1",
  "author": "sheldon",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dynamic-graphics-inc/dgts.git",
    "directory": "templates/template-lib-tsup"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=14.18"
  },
  "scripts": {
    "gen": "npx buf generate proto",
    "gen:clean": "npx buf generate proto --clean",
    "build-fast": "tsup",
    "build": "pnpm gen && tsup --dts",
    "fmt": "prettier --write src/**/*.{js,jsx,ts,tsx} package.json",
    "lint": "eslint .",
    "test": "vitest run",
    "tsup": "tsup",
    "vitest": "vitest --config vitest.config.ts --run",
    "vitest:watch": "vitest --config vitest.config.ts"
  },
  "devDependencies": {
    "@bufbuild/buf": "^1.27.1",
    "@bufbuild/protobuf": "^1.4.0",
    "@bufbuild/protoc-gen-es": "^1.4.0",
    "@jsse/eslint-config": "^0.0.5",
    "@types/node": "^16.18.58",
    "eslint": "~8.51.0",
    "prettier": "~3.0.3",
    "tsup": "~7.2.0",
    "tsx": "~3.13.0",
    "typescript": "~5.2.2",
    "vitest": "~0.34.6"
  }
}
